{
	"$schema": "./node_modules/rome/configuration_schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"ignore": ["node_modules", "dist", ".svelte-kit"],
		"rules": {
			"recommended": true,
			"style": {
				"noNonNullAssertion": "off"
			}
		}
	},
	"formatter": {
		"enabled": true,
		"ignore": ["node_modules", "dist", ".svelte-kit"]
	}
}
